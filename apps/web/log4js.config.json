{
	"appenders": {
		"file": {
			"type": "file",
			"filename": "tagfs.log"
		},
		"console": {
			"type": "console",
			"layout": {
				"type": "pattern",
				"pattern": "%d{hh:mm:ss} %p %c %m%n"
			}
		}
	},
	"categories": {
